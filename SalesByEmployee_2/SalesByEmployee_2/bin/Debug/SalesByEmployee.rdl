  <DataSets>
    <DataSet Name="SalesByEmployee">
      <Query>
        <DataSourceName>AdventureWorksDataSource</DataSourceName>
        <CommandText>SELECT        E.NationalIDNumber AS 'EmployeeId', P.FirstName, P.LastName, P.Title, E.HireDate, E.VacationHours, E.SickLeaveHours
FROM            HumanResources.Employee AS E INNER JOIN
                         Person.Person AS P ON E.BusinessEntityID = P.BusinessEntityID</CommandText>
      </Query>
      <Fields>
        <Field Name="EmployeeId">
          <DataField>EmployeeId</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
