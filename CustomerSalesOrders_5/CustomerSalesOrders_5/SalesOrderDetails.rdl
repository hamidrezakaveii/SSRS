      <DataSourceReference>AdvantureWorkDataSource</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>7cd55294-4f5a-4065-b008-17f0699a6231</rd:DataSourceID>
