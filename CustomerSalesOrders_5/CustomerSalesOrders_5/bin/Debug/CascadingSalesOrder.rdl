    <DataSet Name="SubcategoryDataSet">
      <Query>
        <DataSourceName>AdvantureWorkDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Category">
            <Value>=Parameters!Category.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT PPS.Name AS 'Subcategory'
FROM            Production.ProductSubcategory AS PPS INNER JOIN
                         Production.ProductCategory AS PPC ON PPS.ProductCategoryID = PPC.ProductCategoryID
WHERE        (PPC.Name = @Category)</CommandText>
      </Query>
      <Fields>
        <Field Name="Subcategory">
          <DataField>Subcategory</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ProductDataSet">
      <Query>
        <DataSourceName>AdvantureWorkDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Category">
            <Value>=Parameters!Category.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Subcategory">
            <Value>=Parameters!Subcategory.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT PP.Name AS 'Product'
FROM            Production.Product AS PP INNER JOIN
                         Production.ProductSubcategory AS PPS ON PP.ProductSubcategoryID = PPS.ProductSubcategoryID INNER JOIN
                         Production.ProductCategory AS PPC ON PPS.ProductCategoryID = PPC.ProductCategoryID
WHERE        (PPC.Name = @Category) AND (PPS.Name = @Subcategory)</CommandText>
      </Query>
      <Fields>
        <Field Name="Product">
          <DataField>Product</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
