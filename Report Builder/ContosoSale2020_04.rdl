UNION SELECT CAST('2020-11-21' AS date) AS SalesDate, CAST(81568. AS money) AS Sales
UNION SELECT CAST('2020-12-15' AS date) AS SalesDate, CAST(45973. AS money) AS Sales
UNION SELECT CAST('2020-12-26' AS date) AS SalesDate, CAST(96357. AS money) AS Sales
UNION SELECT CAST('2020-12-31' AS date) AS SalesDate, CAST(81946. AS money) AS Sales</CommandText>
